<?xml version="1.0" encoding="UTF-8"?>
<xf:BottomBarPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:TemFraldaBarata.Core.Features;assembly=TemFraldaBarata.Core"
    xmlns:xf="clr-namespace:BottomBar.XamarinForms;assembly=BottomBar.XamarinForms"
    x:Class="TemFraldaBarata.Core.Features.MainView">
    <views:DiapersView  BindingContext="{Binding DiapersViewModel}" Icon="icon_diaper.png"/>
    <views:OthersView  BindingContext="{Binding OthersViewModel}" Icon="icon_feeding_bottle.png" />
    <views:SettingsView  BindingContext="{Binding SettingsViewModel}" Icon="icon_picture.png" />
</xf:BottomBarPage>