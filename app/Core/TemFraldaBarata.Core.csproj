    <PackageReference Include="ThriveGmbH.BottomNavigationBar.XF" Version="1.0.2" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Features\" />
    <Folder Include="Features\Base\" />
    <Folder Include="Features\Navigation\" />
    <Folder Include="Features\Main\" />
    <Folder Include="Features\Dialog\" />
    <Folder Include="Features\Settings\" />
    <Folder Include="Features\Diapers\" />
    <Folder Include="Features\Others\" />
    <Folder Include="Features\Offers\" />
  </ItemGroup>
</Project>
