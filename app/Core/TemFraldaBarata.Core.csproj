      <DependentUpon>App.xaml</DependentUpon>
    </Compile>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Features\Diapers\FeedView.xaml.cs">
      <DependentUpon>FeedView.xaml</DependentUpon>
    </Compile>
